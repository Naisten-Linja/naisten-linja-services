{
  "name": "naisten-linja-services",
  "engines": {
    "npm": ">=6 <7",
    "node": ">=14 <15"
  },
  "scripts": {
    "backend:build": "tsc -p tsconfig.backend.json",
    "backend:dev": "nodemon --watch src/backend -x \"tsc -p ./tsconfig.backend.json --noEmit && TS_NODE_PROJECT=tsconfig.backend.json ts-node src/start-backend.ts\"",
    "backend:typecheck": "tsc -p tsconfig.backend.json --noEmit",
    "start-production": "npm run db-migrate up && node build-backend/start-backend.js",
    "start-dev-db": "./start-dev-db",
    "db-migrate": "db-migrate",
    "frontend:build": "react-scripts build",
    "frontend:dev": "PORT={$FRONTEND_PORT} BROWSER=none DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "frontend:typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint ./src",
    "lint:watch": "esw -w ./src/**/*.*",
    "lint:node": "check-node-version --package",
    "dev": "npm run start-dev-db && npm run db-migrate up && concurrently npm:backend:dev npm:frontend:dev npm:lint:watch",
    "start:prod": "npm run db-migrate up && node build-backend/start-backend.js",
    "test": "react-scripts test",
    "build": "concurrently npm:backend:build npm:frontend:build",
    "postinstall": "patch-package",
    "heroku-postbuild": "patch-package && npm run build"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "async": "^3.2.0",
    "body-parser": "^1.19.0",
    "check-node-version": "^4.0.3",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "db-migrate": "0.11.11",
    "db-migrate-pg": "1.2.2",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "express-winston": "^4.0.3",
    "generate-password": "^1.5.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-redis": "^6.0.0",
    "node-fetch": "^2.6.7",
    "npm": "^7.16.0",
    "pg": "^8.2.1",
    "redis": "^3.1.2",
    "shortid": "^2.2.15",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@reach/dialog": "^0.16.2",
    "@reach/router": "^1.3.4",
    "@testing-library/dom": "^8.9.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.4.1",
    "@types/axios": "^0.14.0",
    "@types/connect-redis": "0.0.16",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/express-session": "^1.17.0",
    "@types/express-winston": "^4.0.0",
    "@types/generate-password": "^1.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.5",
    "@types/pg": "^7.14.3",
    "@types/pikaday": "^1.7.4",
    "@types/reach__router": "^1.3.9",
    "@types/react": "^17.0.37",
    "@types/react-datepicker": "^4.1.5",
    "@types/react-dom": "^17.0.11",
    "@types/react-modal": "^3.12.1",
    "@types/redis": "^2.8.29",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "^5.1.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@uiw/react-md-editor": "^3.8.5",
    "axios": "^0.21.2",
    "babel-jest": "26.6.3",
    "concurrently": "^5.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-watch": "^7.0.0",
    "formik": "^2.2.9",
    "http-proxy-middleware": "^1.0.5",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.34",
    "nodemon": "^2.0.4",
    "normalize.css": "^8.0.1",
    "patch-package": "^6.4.7",
    "pikaday": "^1.8.2",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-day-picker": "^7.4.10",
    "react-dom": "^17.0.2",
    "react-idle-timer": "^4.6.4",
    "react-modal": "^3.14.4",
    "react-scripts": "4.0.3",
    "react-select-search": "^2.0.3",
    "rsuite": "^4.8.4",
    "styled-components": "^5.1.0",
    "ts-node": "^10.4.0",
    "turretcss": "^5.2.1",
    "typescript": "^4.4.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!react-select-search)"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  }
}
