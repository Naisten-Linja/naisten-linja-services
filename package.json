{
  "name": "naisten-linja-services",
  "engines": {
    "npm": "9.x",
    "node": "19.x"
  },
  "scripts": {
    "backend:build": "tsc -p tsconfig.backend.json",
    "backend:dev": "nodemon --watch src/backend -e js,json,ts -x \"tsc -p ./tsconfig.backend.json --noEmit && TS_NODE_PROJECT=tsconfig.backend.json ts-node src/start-backend.ts\"",
    "backend:typecheck": "tsc -p tsconfig.backend.json --noEmit",
    "start-production": "npm run db-migrate up && node build-backend/start-backend.js",
    "start-dev-db": "./start-dev-db",
    "start-dev-redis": "./start-dev-redis",
    "db-migrate": "db-migrate",
    "frontend:build": "react-scripts build",
    "frontend:dev": "PORT={$FRONTEND_PORT} BROWSER=none DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "frontend:typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "lint:watch": "concurrently npm:eslint:watch npm:prettier:watch",
    "lint:node": "check-node-version --package",
    "typecheck": "npm run frontend:typecheck && npm run backend:typecheck",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "eslint:watch": "onchange -i \"src/**/*.{js,ts,jsx,tsx}\" -- npm run eslint",
    "prettier": "prettier --check \"src/**/*.{js,ts,jsx,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,ts,jsx,tsx}\"",
    "prettier:watch": "onchange -i \"src/**/*.{js,ts,jsx,tsx}\" -- npm run prettier",
    "dev": "npm run start-dev-db && npm run start-dev-redis && npm run db-migrate up && concurrently npm:backend:dev npm:frontend:dev npm:lint:watch",
    "start:prod": "npm run db-migrate up ",
    "frontend:test": "react-scripts test --testMatch \"**/src/frontend/**/?(*.)+(spec|test).[jt]s?(x)\"",
    "backend:test": "NODE_ENV=test jest -c jest.config.backend.js --forceExit --verbose",
    "test": "npm run frontend:test && npm run backend:test",
    "build": "concurrently npm:backend:build npm:frontend:build",
    "postinstall": "node build-backend/start-backend.js && ./node_modules/.bin/patch-package"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "async": "^3.2.4",
    "body-parser": "^1.19.0",
    "check-node-version": "^4.0.3",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "db-migrate": "0.11.11",
    "db-migrate-pg": "1.2.2",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "express-winston": "^4.0.3",
    "generate-password": "^1.5.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-redis": "^7.0.3",
    "node-cron": "^3.0.1",
    "node-fetch": "^2.6.7",
    "npm": "^8.19.2",
    "pg": "^8.8.0",
    "redis": "4.0.6",
    "shortid": "^2.2.15",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@reach/dialog": "^0.16.2",
    "@testing-library/dom": "^8.9.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.4.1",
    "@types/connect-redis": "^0.0.18",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/express-session": "^1.17.0",
    "@types/generate-password": "^1.5.0",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.5",
    "@types/node-cron": "^3.0.2",
    "@types/pg": "^8.6.5",
    "@types/pikaday": "^1.7.4",
    "@types/reach__router": "^1.3.9",
    "@types/react": "^17.0.37",
    "@types/react-datepicker": "^4.1.5",
    "@types/react-dom": "^17.0.11",
    "@types/react-modal": "^3.12.1",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "^5.1.0",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.5",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@uiw/react-md-editor": "^3.20.5",
    "axios": "^0.21.2",
    "babel-jest": "26.6.3",
    "concurrently": "^7.3.0",
    "copy-to-clipboard": "^3.3.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-watch": "^8.0.0",
    "formik": "^2.2.9",
    "http-proxy-middleware": "^1.0.5",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-http-backend": "^1.4.1",
    "jest": "^29.0.3",
    "jest-extended": "^3.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "nodemon": "^2.0.19",
    "normalize.css": "^8.0.1",
    "onchange": "^7.1.0",
    "patch-package": "^6.4.7",
    "pikaday": "^1.8.2",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-data-table-component": "^7.5.2",
    "react-day-picker": "^7.4.10",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.5",
    "react-icons": "^4.4.0",
    "react-idle-timer": "^4.6.4",
    "react-modal": "^3.14.4",
    "react-router-dom": "^6.4.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.4.0",
    "rehype-sanitize": "^5.0.1",
    "rsuite": "^4.8.4",
    "styled-components": "^5.1.0",
    "supertest": "^6.2.4",
    "testcontainers": "^8.13.2",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "ts-node-test": "^0.1.1",
    "turretcss": "^5.2.1",
    "typescript": "^4.8.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  }
}
