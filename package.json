{
  "name": "naisten-linja-services",
  "engines": {
    "npm": ">=6 <7",
    "node": ">=12 <13"
  },
  "scripts": {
    "dev": "concurrently \"npm:backend-dev\" \"npm:frontend-dev\"",
    "backend-build": "npm run lint-node && tsc --project tsconfig-backend.json",
    "backend-dev": "nodemon --watch 'src/backend/**/*.ts' -x TS_NODE_PROJECT=tsconfig-backend.json ts-node src/index-backend.ts",
    "backend-prod-start": "ENV=prod npm run db-migrate up && node build-backend/index-backend.js",
    "frontend-dev": "PORT=3000 BROWSER=none react-scripts start",
    "frontend-build": "npm run lint-node && REACT_APP_DISCOURSE_URL=\"https://ryhmat.naistenlinja.fi\" react-scripts build",
    "db-migrate": "db-migrate",
    "lint-node": "check-node-version --package",
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "npm run backend-build && npm run frontend-build",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "async": "^3.2.0",
    "body-parser": "^1.19.0",
    "check-node-version": "^4.0.3",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "express-session": "^1.17.1",
    "express-winston": "^4.0.3",
    "generate-password": "^1.5.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.2.1",
    "shortid": "^2.2.15",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@reach/router": "^1.3.3",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/express-session": "^1.17.0",
    "@types/express-winston": "^4.0.0",
    "@types/generate-password": "^1.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.5",
    "@types/pg": "^7.14.3",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "^5.1.0",
    "@types/winston": "^2.4.4",
    "axios": "^0.19.2",
    "concurrently": "^5.3.0",
    "http-proxy-middleware": "^1.0.5",
    "nodemon": "^2.0.4",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "react-select-search": "^2.0.3",
    "styled-components": "^5.1.0",
    "ts-node": "^8.10.1",
    "turretcss": "^5.2.1",
    "typescript": "^3.9.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
